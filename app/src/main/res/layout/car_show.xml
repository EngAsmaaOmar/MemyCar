<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/frcar_img"
        android:layout_width="386dp"
        android:layout_height="182dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fristcar" />

    <ImageView
        android:id="@+id/car_location"
        android:layout_width="30dp"
        android:layout_height="34dp"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toStartOf="@+id/carspeed_txt"
        app:layout_constraintTop_toBottomOf="@+id/carshow_txt"
        app:srcCompat="@android:mipmap/sym_def_app_icon"
        android:layout_marginRight="20dp" />

    <ImageView
        android:id="@+id/car_speed"
        android:layout_width="34dp"
        android:layout_height="36dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toStartOf="@+id/frcarloc_txt"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carshow_txt"
        app:srcCompat="@android:mipmap/sym_def_app_icon"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/carshow_txt"
        android:layout_width="328dp"
        android:layout_height="45dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/cartxtmark"
        android:textColor="@android:color/holo_blue_light"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frcar_img" />

    <ImageView
        android:id="@+id/car_date"
        android:layout_width="31dp"
        android:layout_height="29dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/car_speed"
        app:srcCompat="@android:mipmap/sym_def_app_icon" />

    <ImageView
        android:id="@+id/car_speed_hr"
        android:layout_width="30dp"
        android:layout_height="31dp"
        android:layout_marginBottom="12dp"
        android:layout_marginStart="92dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintStart_toEndOf="@+id/carloc_txt"
        app:srcCompat="@android:mipmap/sym_def_app_icon"
        android:layout_marginLeft="92dp" />

    <TextView
        android:id="@+id/frcarloc_txt"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginEnd="84dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintEnd_toStartOf="@+id/car_location"
        app:layout_constraintTop_toBottomOf="@+id/carshow_txt"
        android:layout_marginRight="84dp" />

    <TextView
        android:id="@+id/carspeed_txt"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginEnd="48dp"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carshow_txt"
        android:layout_marginRight="48dp" />

    <TextView
        android:id="@+id/carloc_txt"
        android:layout_width="73dp"
        android:layout_height="21dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="12dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintStart_toEndOf="@+id/car_date"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/carspeed_hr_txt"
        android:layout_width="72dp"
        android:layout_height="23dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="44dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toEndOf="@+id/car_speed_hr"
        app:layout_constraintTop_toBottomOf="@+id/carspeed_txt" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="385dp"
        android:layout_height="178dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/seccar" />

</android.support.constraint.ConstraintLayout>